.video-container {
  width: 100%;
  height: 100%;
  background: white;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 999;
  /* display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column; */
  transform: rotate(90%);
}

.video-container video {
  width: 96%;
  height: 422rpx;
  background: white;
  position: fixed;
  top: 40%;
  left: 50%;
  transform: translate(-50%, -50%);
}

.video-container button {
  display: block;
  width: 250rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  color: white;
  font-size: 32rpx;
  background-image: linear-gradient(180deg, #8cd100 0%, #509f00 100%);
  box-shadow: 0px 4rpx 0px 0px #4f8701;
  border-radius: 22rpx;
  margin: 60rpx 10rpx 0 10rpx;
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  margin-top:280rpx;
}

.fixed-top {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 66;
}

.header {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 139rpx;
  z-index: -1;
}

.header image {
  width: 100%;
  height: 139rpx;
}

.title {
  font-size: 64rpx;
  padding-left: 15rpx;
  margin-bottom: 15rpx;
  width: 60%;
}

.tab {
  width: 700rpx;
  height: 80rpx;
  background-color: #fff;
  box-shadow: 0px 2rpx 15rpx 0px rgba(220, 220, 220, 0.5);
  border-radius: 40rpx;
  margin-left: 25rpx;
}

.tab view {
  float: left;
  color: #848484;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 30rpx;
  margin-left: 75rpx;
  position: relative;
}

.tab view .text {
  padding-bottom: 6rpx;
  border-bottom: 4rpx solid white;
}

.tab view.active .text {
  border-bottom: 4rpx solid #fde214;
}

.tab view .span {
  background-color: #ff7138;
  border-radius: 50%;
  width: 30rpx;
  height: 30rpx;
  line-height: 30rpx;
  text-align: center;
  font-size: 20rpx;
  color: #fff;
  position: absolute;
  top: 5rpx;
  right: -25rpx;
}

.list .item {
  width: 710rpx;
  height: 373rpx;
  background-color: #fff;
  box-shadow: 0px 3rpx 18rpx 0px #e4e4e4;
  border-radius: 10rpx;
  margin: 20rpx 0 0 20rpx;
}

.list .item .top {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: 2rpx solid #f1f1f1;
  display: flex;
  justify-content: space-between;
  padding: 0 20rpx;
}

.view-video {
  width: 80rpx;
  text-align: center;
  line-height: 100%;
  margin-left: 20rpx;
}

.view-video image {
  width: 50rpx;
  height: 33rpx;
  line-height: 33rpx;
  margin-bottom: 10rpx;
  margin-top: 18rpx;
}

.view-video .text {
  height: 20rpx;
  line-height: 20rpx;
  font-size: 20rpx;
}

.list .item .con {
  width: 100%;
  height: 273rpx;
  display: flex;
  justify-content: space-around;
}

.list .item .con .view {
  margin-top: 40rpx;
}

.list .item .con .view .text {
  display: block;
  text-align: center;
  margin-top: 8rpx;
  font-size: 55rpx;
}

.list .item .con image {
  width: 157rpx;
  height: 157rpx;
}

.list .item .con .image {
  width: 157rpx;
  height: 157rpx;
  position: relative;
}

.list .span, .other-list .span {
  background-color: #ff7138;
  border-radius: 50%;
  width: 42rpx;
  height: 42rpx;
  line-height: 42rpx;
  text-align: center;
  font-size: 24rpx;
  color: #fff;
  position: absolute;
  right: -20rpx;
  top: -20rpx;
}

.other-list .item {
  width: 710rpx;
  height: 210rpx;
  background-color: #fff;
  box-shadow: 0 3rpx 18rpx 0 #e4e4e4;
  border-radius: 10rpx;
  margin: 20rpx 0 0 20rpx;
  padding: 26rpx;
  display: flex;
}

.other-list .item .left {
  width: 157rpx;
  height: 157rpx;
  position: relative;
}

.other-list .item .left image {
  width: 157rpx;
  height: 157rpx;
}

.other-list .item .right {
  flex: 1;
  padding-left: 20rpx;
}

.other-list .item .right .status {
  width: 63rpx;
  height: 38rpx;
  line-height: 38rpx;
  text-align: center;
  border-radius: 8rpx;
  font-size: 24rpx;
  color: white;
}

.other-list .item.pre .right .status {
  background-color: #ffc439;
}

.other-list .item.task .right .status {
  background: #7ec500;
}

