.number-index {
  width: 74rpx;
  height: 60rpx;
  line-height: 60rpx;
  background-color: #ff7c00;
  border-radius: 30rpx;
  border-top-left-radius: 0;
  border-bottom-left-radius: 0;
  font-size: 48rpx;
  color: #fff;
  float: left;
  padding-left: 5rpx;
}

.complete-btn {
  width: 380rpx;
  height: 97rpx;
  line-height: 97rpx;
  text-align: center;
  position: relative;
  color: white;
  margin: 60rpx auto;
  box-shadow: 0 6rpx 10rpx #e8b4ab;
  border-radius: 48.5rpx;
}

.complete-btn image {
  width: 380rpx;
  height: 97rpx;
  position: absolute;
  left: 0;
  top: 0;
  z-index: -1;
}

.top {
  width: 100%;
  height: 98rpx;
  padding-top: 2rpx;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 77;
}

.top .image {
  width: 100%;
  height: 98rpx;
  position: absolute;
  left: 0;
  top: 0;
  z-index: -1;
}

.top .cup-container {
  position: absolute;
  right: 0;
  top: 0;
}

.title {
  height: 60rpx;
  line-height: 60rpx;
  margin-left: 20rpx;
  font-size: 64rpx;
  position: relative;
  top: 10rpx;
}

.lesson-page-question-container {
  display: flex;
  width: 100%;
  padding-left: 20rpx;
}

.lesson-page-question-container .left {
  flex: 1;
}

.lesson-page-question-container .right {
  width: 156rpx;
  margin-left: 10rpx;
}

.lesson-page-question-container.label .right {
  width: 134rpx;
  margin-left: 25rpx;
}

.committed-container {
  width: 257rpx;
  height: 90rpx;
  position: fixed;
  left: 50%;
  bottom: 100rpx;
  transform: translateX(-50%);
  z-index: 19;
}
