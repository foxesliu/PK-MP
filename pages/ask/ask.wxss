@import '/public/style/sanfeng.wxss';
@import '/public/style/pkicon.wxss';

.committed-container {
  margin-top: 30rpx;
  display: flex;
  justify-content: center;
}

.bottom-btn-container {
  width: 100%;
  position: fixed;
  bottom: 100rpx;
  left: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 77;
}

.bottom-btn-container .btn {
  width: 214rpx;
  height: 66rpx;
  background-color: #fff200;
  box-shadow: 0px 6rpx 0px 0px #ffc900;
  border-radius: 33rpx;
}

.bottom-btn-container .btn .left {
  width: 56rpx;
  height: 56rpx;
  line-height: 56rpx;
  text-align: center;
  background-color: #fff200;
  box-shadow: inset 0px 1rpx 12rpx 0px #ffc100;
  border-radius: 50%;
  margin: 5rpx 0 0 8rpx;
}

.bottom-btn-container .btn .left .pkicon {
  display: inline-block;
  width: 56rpx;
  height: 56rpx;
  line-height: 56rpx;
  text-align: center;
  font-size: 25rpx;
  color: #ff7400;
  transform: translate(-2rpx, 2rpx);
}

.bottom-btn-container .btn .txt {
  height: 56rpx;
  line-height: 56rpx;
  margin: 5rpx 0 0 10rpx;
}

page {
  background: black;
}
