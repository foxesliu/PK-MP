@import '/public/style/sanfeng.wxss';
@import '/public/style/pkicon.wxss';

.view {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.8);
  position: fixed;
  left: 0;
  top: 0;
  z-index: 88;
}

.box {
  position: fixed;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}

.image {
  width: 490rpx;
  height: 8rpx;
  margin-top: 30rpx;
}
